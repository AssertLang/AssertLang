{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://promptware.dev/schemas/tools/logger.v1.json",
  "type": "object",
  "properties": {
    "level": {
      "type": "string",
      "enum": [
        "debug",
        "info",
        "warn",
        "error"
      ]
    },
    "message": {
      "type": "string"
    },
    "context": {
      "type": "object"
    }
  },
  "required": [
    "level",
    "message"
  ],
  "additionalProperties": false
}